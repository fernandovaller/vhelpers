{
    "name": "fernandovaller/vhelpers",
    "description": "Helpers usados nos sistemas",
    "keywords": [
        "Helpers"
    ],
    "homepage": "https://www.fernandovaller.com",
    "license": "MIT",
    "authors": [{
        "name": "Fernando Valler",
        "email": "fernandovaller@gmail.com"
    }],
    "autoload": {
        "psr-4": {
            "FVCode\\VHelpers\\": "src"
        }
    },
    "scripts": {
        "test": "vendor\\bin\\phpunit",
        "fixer-check": "vendor\\bin\\php-cs-fixer fix --diff --verbose --dry-run",
        "fixer-run": "vendor\\bin\\php-cs-fixer fix"
    },
    "config": {
        "platform": {
            "php": "5.6"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpunit/dbunit": "^2.0"
    }
}